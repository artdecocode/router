{
  "name": "@depack/router",
  "version": "1.0.4",
  "description": "The Preact Router Component For Compilation With Depack.",
  "main": "src/index.jsx",
  "module": "build/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "b": "alamode src -o build -s -j -p",
    "d": "yarn-s d1",
    "d1": "NODE_DEBUG=doc doc src/index.js -g",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "e": "node example",
    "example/": "yarn e example/example",
    "depack": "depack-dev example/App.jsx -o splendid/js/main.js -I 2018 -a -H -p -w"
  },
  "files": [
    "src",
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dpck/router.git"
  },
  "keywords": [
    "router",
    "depack",
    "preact",
    "closure"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpck/router/issues"
  },
  "homepage": "https://github.com/dpck/router#readme",
  "devDependencies": {
    "@depack/render": "^1.1.4",
    "@idio/core": "1.4.0",
    "alamode": "2.2.0",
    "documentary": "1.27.2",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "4.1.1-alpha"
  }
}
